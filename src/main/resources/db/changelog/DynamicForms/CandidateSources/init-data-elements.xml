			<column name='default_value' value="NULL" />
			<column name='visibility' valueBoolean="true" />
			<column name='layout_id' valueComputed="(select id from df_layout where name='CreateCandidateSource' and type='Create')" />
		</insert>
		
	</changeSet>
	
