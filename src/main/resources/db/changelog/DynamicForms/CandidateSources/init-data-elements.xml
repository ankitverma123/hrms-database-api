			<column name='api_query' value='NULL' />
			<column name='control_type' value='matInput'  />
			<column name='dependent_on' value='NULL' />
			<column name='is_dependent' value='NULL' />
			<column name='label' value='Create new candidate source' />
			<column name='show_label' valueBoolean="true" />
